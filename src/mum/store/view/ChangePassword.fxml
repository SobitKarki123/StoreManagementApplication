<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="210.0" prefWidth="318.0" xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1" fx:controller="mum.store.controller.PasswordController">
   <children>
      <Label layoutX="87.0" layoutY="14.0" text="Change Password">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
      </Label>
      <Label layoutX="23.0" layoutY="52.0" text="Old Password" />
      <Label layoutX="20.0" layoutY="106.0" text="New Password" />
      <Button layoutX="126.0" layoutY="152.0" mnemonicParsing="false" onAction="#changePassword" prefHeight="25.0" prefWidth="66.0" text="Save" />
      <Button layoutX="221.0" layoutY="152.0" mnemonicParsing="false" onAction="#clearItem" prefHeight="25.0" prefWidth="66.0" text="Clear" />
      <PasswordField fx:id="newPassword" layoutX="123.0" layoutY="102.0" prefHeight="25.0" prefWidth="164.0" />
      <PasswordField fx:id="oldPassword" editable="false" layoutX="123.0" layoutY="48.0" prefHeight="25.0" prefWidth="164.0" />
   </children>
</AnchorPane>
